#include <iostream>
#include <string>
using namespace std;

int main()
{
    string frame;
    getline(cin, frame);
    int count = 0;
    string str;
    cout << "**********" << endl;
    for (int i = 0; i < frame.length(); i++)
    {

        if (frame[i] != ' ')
        {
            // cout << "* ";
            str = frame[i];
            // cout <<" *";
            count++;
            cout<<str;
        }
        else
        {
            cout << "\n";
        }
    }

    cout << "\n**********" << endl;
    return 0;
}
